<!--miniprogram/pages/result/result.wxml-->

<cu-custom bgImage="" bgColor="bg-gradual-orange" isBack="{{true}}">
<view slot="backText" style="color:white;">返回</view>
  <view slot="content" style="color:white;">E招会</view>
</cu-custom>
<block wx:for="{{result_content}}" wx:for-item="item">
